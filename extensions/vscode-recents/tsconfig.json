{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 16",
	"include": ["src/**/*", "vicinae-env.d.ts"],
	"compilerOptions": {
		"lib": ["es2022"],
		"module": "commonjs",
		"target": "es2022",
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"jsx": "react-jsx"
	}
}
