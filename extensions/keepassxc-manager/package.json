{
   "name": "keepassxc",
   "title": "KeePassXC",
   "version": "1.0.0",
   "description": "Search passwords, usernames, TOTP codes, and more using KeePassXC directly from Vicinae.",
   "icon": "keepassxc.webp",
   "main": "dist/index.js",
   "scripts": {
      "build": "vici build",
      "develop": "vici develop"
   },
   "keywords": [
      "keepassxc",
      "password",
      "security"
   ],
   "categories": [
      "Security"
   ],
   "author": "peks",
   "license": "MIT",
   "dependencies": {
      "@vicinae/api": "^0.18.3",
      "csv-parse": "^6.1.0"
   },
   "commands": [
      {
         "name": "search",
         "title": "Search KeePassXC",
         "description": "Search passwords, usernames, TOTP codes, and more using KeePassXC directly from Vicinae.",
         "mode": "view",
         "preferences": [
            {
               "name": "database",
               "type": "file",
               "required": true,
               "title": "KeePassXC Database File",
               "description": "Select your KeePassXC database file (.kdbx)"
            },
            {
               "name": "lockAfterInactivity",
               "type": "dropdown",
               "required": false,
               "default": "0",
               "title": "Lock Database After Inactivity",
               "description": "Automatically lock the database after a period of inactivity (in minutes)",
               "data": [
                  {
                     "title": "Never",
                     "value": "0"
                  },
                  {
                     "title": "1 minute",
                     "value": "1"
                  },
                  {
                     "title": "5 minutes",
                     "value": "5"
                  },
                  {
                     "title": "10 minutes",
                     "value": "10"
                  },
                  {
                     "title": "30 minutes",
                     "value": "30"
                  },
                  {
                     "title": "60 minutes",
                     "value": "60"
                  }
               ]
            }
         ]
      }
   ]
}
