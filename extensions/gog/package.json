  "dependencies": {
    "@vicinae/api": "^0.19.3"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.11.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^27.4.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  }
}
