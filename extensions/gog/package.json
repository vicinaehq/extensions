{
  "name": "gog",
  "title": "gog",
  "version": "1.0.0",
  "description": "Manage Gmail, Calendar, Drive, Docs, Sheets, Slides, Tasks, and Contacts using the gog CLI",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "author": "knoopx",
  "contributors": [],
  "pastContributors": [],
  "icon": "extension_icon.png",
  "commands": [
    {
      "name": "gmail",
      "title": "Gmail",
      "description": "Manage Gmail",
      "mode": "view",
      "icon": "gmail.png"
    },
    {
      "name": "calendar",
      "title": "Calendar",
      "description": "View upcoming events (next 30 days)",
      "mode": "view",
      "icon": "calendar.png"
    },
    {
      "name": "drive",
      "title": "Drive",
      "description": "Manage Drive",
      "mode": "view",
      "icon": "drive.png"
    },
    {
      "name": "docs",
      "title": "Docs",
      "description": "Manage Docs",
      "mode": "view",
      "icon": "docs.png"
    },
    {
      "name": "sheets",
      "title": "Sheets",
      "description": "Manage Sheets",
      "mode": "view",
      "icon": "sheets.png"
    },
    {
      "name": "slides",
      "title": "Slides",
      "description": "Manage Slides",
      "mode": "view",
      "icon": "slides.png"
    },
    {
      "name": "tasks",
      "title": "Tasks",
      "description": "Manage Tasks",
      "mode": "view",
      "icon": "tasks.png"
    },
    {
      "name": "contacts",
      "title": "Contacts",
      "description": "Manage Contacts",
      "mode": "view",
      "icon": "contacts.png"
    }
