{
  "$schema": "https://raw.githubusercontent.com/vicinaehq/vicinae/refs/heads/main/extra/schemas/extension.json",
  "name": "html-symbol-finder",
  "title": "HTML Symbol Finder",
  "description": "Search HTML symbols",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "author": "codingcodax",
  "contributors": [],
  "pastContributors": [],
  "icon": "extension_icon.png",
  "commands": [
    {
      "name": "index",
      "title": "Search HTML Symbols",
      "description": "Browse and copy-paste HTML Symbols",
      "mode": "view"
    }
  ],
  "scripts": {
    "build": "vici build",
    "build:dataset": "ts-node src/scripts/generate-dataset.ts",
    "build:html-list": "ts-node src/scripts/generate-html-list.ts",
    "dev": "vici develop",
    "lint": "vici lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@vicinae/api": "^0.16.1",
    "fuse.js": "^7.1.0"
  },
  "devDependencies": {
    "@raycast/api": "^1.103.6",
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "unidata16": "^1.1.0"
  }
}
